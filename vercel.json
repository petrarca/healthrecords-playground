{
  "version": 2,
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "pnpm build",
        "outputDirectory": "dist"
      }
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}